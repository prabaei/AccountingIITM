@model ImprestProjectDetails




<div class="row">
    <div class="form-group">
        <label class="col-md-2">Project NO :</label>
        <label class="col-md-4">@Model.ProjectNo</label>
    </div>
</div>
<div class="row">
    <div class="form-group">
        <label class="col-md-2">Start Date :</label>
        <label class="col-md-4">@Model.ProjectStartDate</label>
    </div>
</div>
<div class="row">
    <div class="form-group">
        <label class="col-md-2">Close Date :</label>
        <label class="col-md-4">@Model.ProjectCloseDate</label>
    </div>
</div>

