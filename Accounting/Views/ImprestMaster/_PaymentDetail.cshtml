@model paymentDetails


<div class="row">
    <div class="form-group">
        <label class="col-md-2">Voucher NO :</label>
        <label class="col-md-4">@Model.VoucherNo</label>
    </div>
</div>
<div class="row">
    <div class="form-group">
        <label class="col-md-2">Date Of Drawn :</label>
        <label class="col-md-4">@Model.dateofDrawn</label>
    </div>
</div>
<div class="row">
    <div class="form-group">
        <label class="col-md-2">Amount Paid :</label>
        <label class="col-md-4">@Model.AmountPayed</label>
    </div>
</div>